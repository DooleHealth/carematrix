let EmailPage = class EmailPage {
    constructor(fb, authService, router, loadingController, translate, alertController) {
        this.fb = fb;
        this.authService = authService;
        this.router = router;
        this.loadingController = loadingController;
        this.translate = translate;
        this.alertController = alertController;
        this.pseudo_email = "";
    }
    ngOnInit() {
        this.setCredentials();
        // Based on user action, get API resource
        switch (this.authService.action) {
            case 'signUpUser':
                this.resource = 'registre3';
                this.route = 'identity';
                this.header = 'landing.registre';
                break;
            case 'passwordRecovery':
                this.resource = 'contrasenya2';
                this.route = 'email';
                this.header = 'landing.password';
                break;
            case 'addSocialAuth':
                this.resource = 'credencial2';
                this.route = 'email';
                this.header = 'landing.social';
                break;
        }
    }
    submit() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const loading = yield this.loadingController.create();
            yield loading.present();
            console.log("this.authService.is_recovery: ", this.resource);
            this.authService.validateCredentials(this.credentials.value, this.resource).subscribe((data) => Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
                yield loading.dismiss();
                console.log(data);
                if (this.authService.action == "addSocialAuth") {
                    this.router.navigate(['confirmation']);
                }
                else {
                    this.authService.id_usuari_mgc = data.id_usuari_mgc;
                    this.router.navigate(['/password']);
                }
            }), (error) => {
                // Called when error
                loading.dismiss();
                this.showAlert(this.translate.instant('login.fail'));
            }, () => {
                // Called when operation is complete (both success and error)
                loading.dismiss();
            });
        });
    }
    showAlert(message) {
        this.alertController.create({
            header: 'Error',
            message: message,
            buttons: ['OK']
        }).then(res => {
            res.present();
        });
    }
    setCredentials() {
        // hide partially email address
        let hideEmail = function (email) {
            return email.replace(/(.{2})(.*)(?=@)/, function (gp1, gp2, gp3) {
                for (let i = 0; i < gp3.length; i++) {
                    gp2 += "*";
                }
                return gp2;
            });
        };
        this.pseudo_email = hideEmail(this.authService.email);
        // Prepare API request parameters (recovery or sign-up)
        if (this.authService.action == "passwordRecovery") {
            this.credentials = this.fb.group({
                id_usuari_mgc: this.authService.id_usuari_mgc,
                email: this.authService.email,
                codi: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required]]
            });
        }
        else if (this.authService.action == "addSocialAuth") {
            this.credentials = this.fb.group({
                mutua: this.authService.mutua,
                credencial: "google",
                codi: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required]],
                username: this.authService.user.username
            });
        }
        else {
            this.credentials = this.fb.group({
                mutua: this.authService.mutua,
                codi: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__["Validators"].required]]
            });
        }
    }
};
