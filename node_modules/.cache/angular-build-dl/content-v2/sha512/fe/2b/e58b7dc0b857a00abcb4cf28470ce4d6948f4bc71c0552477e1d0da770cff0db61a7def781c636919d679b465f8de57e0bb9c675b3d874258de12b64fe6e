TELEFON_1 }} </ion-row>\\n      <ion-grid>\\n        <ion-row>\\n          <ion-col size=\\\"4\\\" style=\\\"margin-left: -4%;\\\">\\n            <div>\\n              <ion-button  class=\\\"cuadreIcon\\\" > {{ 'centers.map'|translate|uppercase }} </ion-button>\\n            </div>\\n          </ion-col>\\n          <ion-col>\\n            <div>\\n              <ion-button  class=\\\"cuadreIcon\\\">\\n                {{ 'centers.call'|translate|uppercase }}\\n              </ion-button>\\n            </div>\\n          </ion-col>\\n          <ion-col>\\n            <div>\\n              <div *ngIf=\\\"canScheduleAnAppointment(item)\\\">\\n                <ion-button   class=\\\"cuadreIcon\\\" (click)=\\\"getAppointment(item.CITA_ONLINE)\\\">\\n                  {{ 'centers.appointment' | translate | uppercase }}\\n                </ion-button>\\n              </div>\\n            </div>\\n          </ion-col>\\n        </ion-row>\\n      </ion-grid>\\n    </ion-card-content>\\n  </ion-card>\\n</ng-container>\\n</div> \\n</ion-content>\\n\";","import { NgModule, CUSTOM_ELEMENTS_SCHEMA } from '@angular/core';\nimport { CommonModule } from '@angular/common';\nimport { FormsModule, ReactiveFormsModule } from '@angular/forms';\n\nimport { IonicModule } from '@ionic/angular';\n\nimport { ContactPageRoutingModule } from './contact-routing.module';\n\nimport { ContactPage } from './contact.page';\nimport { TranslateModule } from '@ngx-translate/core';\nimport { CallNumber } from '@ionic-native/call-number/ngx';\nimport { ComponentsModule } from 'src/app/components/components.module';\n\n@NgModule({\n  imports: [\n    CommonModule,\n    FormsModule,\n    IonicModule,\n    ReactiveFormsModule,\n    ContactPageRoutingModule,\n    TranslateModule,\n    ComponentsModule\n  ],\n  providers:[CallNumber],\n  declarations: [ContactPage]\n})\nexport class ContactPageModule {}\n","import { Component, OnInit } from '@angular/core';\nimport { Router } from '@angular/router';\nimport { LoadingController } from '@ionic/angular';\nimport { LanguageService } from 'src/app/pages/language/language.service';\nimport { AppointmentService } from 'src/app/services/appointment.service';\nimport { HealthcareService } from 'src/app/services/healthcare.service';\nimport { CenterCardRequest, DoctorCardRequest, Provider, ProviderByLocation } from 'src/app/shared/classes/credential-data';\nimport { getRandomBackground } from 'src/app/shared/classes/utils';\n\n@Component({\n  selector: 'app-contact',\n  templateUrl: './contact.page.html',\n  styleUrls: ['./contact.page.scss'],\n})\nexport class ContactPage implements OnInit {\n  backgroundImage: string = getRandomBackground();\n\n  providerCards: Provider[];\n  constructor(private healthcareService: HealthcareService,\n    private router: Router,\n    private loadingController: LoadingController,\n    private appointments: AppointmentService,\n    public languageService: LanguageService) { }\n\n\n  ngOnInit() {\n    this.getProvidersCard(this.healthcareService.providerCard)\n  }\n\n  public getProvidersCard(provider: Provider) {\n\n    if(provider === undefined)\n      this.router.navigateByUrl('/healthcare/providers', { replaceUrl: true });\n\n    return provider.isDoctor() ? this.getProvider(provider.CEN) : this.getMedicalCenter(provider.CODI_CENTR, this.healthcareService.specialty.CODI_SERV);\n  }\n\n\n  async getMedicalCenter(codi_centre: string, specialty: string) {\n\n    const request = new CenterCardRequest(this.languageService.getLanguageAPI(), this.healthcareService.quadre, codi_centre, specialty);\n\n    const loading = await this.loadingController.create();\n    await loading.present();\n\n    this.healthcareService.getMedicalCenter(request).subscribe(\n      async (data: Provider[]) => {\n        await loading.dismiss();\n        this.providerCards = data;\n      },\n      (error) => {\n        loading.dismiss();\n       throw error;\n      },\n      () => {\n        loading.dismiss();\n      });\n  }\n\n  async getProvider(codi_metge: string) {\n\n    const request = new DoctorCardRequest(this.languageService.getLanguageAPI(), this.healthcareService.quadre, codi_metge);\n\n    const loading = await this.loadingController.create();\n    await loading.present();\n\n    this.healthcareService.getDoctorCard(request).subscribe(\n      async (data: Provider[]) => {\n        await loading.dismiss();\n        this.providerCards = data;\n      },\n      (error) => {\n        loading.dismiss();\n       throw error;\n      },\n      () => {\n        loading.dismiss();\n      });\n  }\n\n  public canScheduleAnAppointment(providerCard : Provider){\n   \n    return providerCard.isAvailableForAppointment();\n  }\n\n  getAppointment(id_agenda : string){\n   \n    this.appointments.id_agenda = id_agenda;\n    this.appointments.codi_servei = this.healthcareService.specialty.CODI_SERV;\n    console.log(\" this.healthcareService.specialty.CODI_SERV; \", this.healthcareService.specialty.CODI_SERV);\n    this.router.navigateByUrl('/healthcare/appointments', { replaceUrl:true }); \n   \n  }\n}\n","import { NgModule } from '@angular/core';\nimport { CommonModule } from '@angular/common';\nimport { FormsModule } from '@angular/forms';\n\nimport { IonicModule } from '@ionic/angular';\n\nimport { ContactPageRoutingModule } from './contact-routing.module';\n\nimport { ContactPage } from './contact.page';\nimport { TranslateModule } from '@ngx-translate/core';\nimport { Ng2SearchPipeModule } from 'ng2-search-filter';\nimport { ComponentsModule } from 'src/app/components/components.module';\n\n\n@NgModule({\n  imports: [\n    CommonModule,\n    FormsModule,\n    IonicModule,\n    ContactPageRoutingModule,\n    TranslateModule,\n    Ng2SearchPipeModule,\n    ComponentsModule\n  ],\n  declarations: [ContactPage]\n})\nexport class ContactPageModule {}\n","export default \"ion-item-divider {\\n  background-color: #fdfdfc;\\n  margin-bottom: 15px;\\n  padding: 3%;\\n  border-radius: 10px;\\n  box