 = [];\n  var startTime: Date;\n  var endTime: Date;\n  // Iterate for each day\n  this.appointmentService.schedules.forEach( (element) => {\n    \n    // convert API date to format YYYY-MM-DD\n    var pattern = /(\\d{4})(\\d{2})(\\d{2})/;\n    var current = new Date(element.data.replace(pattern, '$1-$2-$3')); \n\n    // Iterate for each available hour\n    element.horaris.forEach( (item) => {\n\n      // Check if it's available\n      if(item.disponible){\n        \n        startTime = new Date(current);\n          // Time format response is \"HHMM\", so we extracts HH and MM\n        startTime.setHours(parseInt(item.hora.substring(0,2)));\n        startTime.setMinutes(parseInt(item.hora.substring(2,4)));\n  \n        // Set end time 15 minutes after start time\n        endTime = new Date(startTime);\n        endTime.setMinutes(startTime.getMinutes() + time_span);\n\n        // Add the time slot to calendar events array\n        events.push({\n            title: 'Day - ' + startTime.toDateString() ,\n            startTime: startTime,\n            endTime: endTime,\n            allDay: false\n        });\n  \n      }\n      \n    });\n   \n});\n  \n  return events;\n}\n\naddTeladocScheduleToCalendar(): any[] {\n  var events = [];\n  var startTime: Date;\n  var endTime: Date;\n  // Iterate for each day\n  this.appointmentService.teladocSchedules.forEach( (element) => {\n    \n    var current = new Date(element.date); \n\n    // Iterate for each available hour\n    element.day_slots.forEach( (item) => {\n\n      // Check if it's available\n      if(item.availability){\n        \n        startTime = new Date(current);\n          // Time format response is \"HHMM\", so we extracts HH and MM\n        startTime.setHours(parseInt(item.time_ini.substring(0,2)));\n        startTime.setMinutes(parseInt(item.time_ini.substring(3,5)));\n  \n        // Set end time 15 minutes after start time\n        endTime = new Date(startTime);\n        endTime.setMinutes(startTime.getMinute