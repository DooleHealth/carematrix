let AuthorizationsPage = class AuthorizationsPage {
    constructor(loadingController, service, auth, router, datepipe, fb) {
        this.loadingController = loadingController;
        this.service = service;
        this.auth = auth;
        this.router = router;
        this.datepipe = datepipe;
        this.fb = fb;
        this.backgroundImage = Object(src_app_shared_classes_utils__WEBPACK_IMPORTED_MODULE_10__["getRandomBackground"])();
    }
    ngOnInit() {
        if (this.service.authorizationFile === undefined)
            this.router.navigate(['app/home/healthcare/authorizations/file']);
        this.form = this.fb.group({
            data_prestacio: [''],
            centre: [''],
            cp: [''],
            observacions: ['']
        });
    }
    /**
     * setAuthorization
     */
    setAuthorization() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            let date = this.form.get('data_prestacio').value;
            if (date) {
                var current = new Date(date.split('T')[0]);
                let data_prestacio = this.datepipe.transform(current, 'dd/MM/YYYY');
                this.form.get('data_prestacio').setValue(data_prestacio);
            }
            let params = this.form.value;
            params.auto = this.service.authorizationFile;
            params.mutua = this.auth.user.mutua;
            params.idioma = this.auth.user.idioma;
            params.mutua = this.auth.user.mutua;
            const loading = yield this.loadingController.create();
            yield loading.present();
            return this.service.setAuthorization(params).subscribe((data) => Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
                yield loading.dismiss();
                console.log("response: ", data);
                this.router.navigate(['app/home/healthcare/authorizations/success']);
            }), (error) => {
                // Called when error
                loading.dismiss();
                throw error;
            }, () => {
                // Called when operation is complete (both success and error)
                loading.dismiss();
            });
        });
    }
    openCalendar() {
    }
};
