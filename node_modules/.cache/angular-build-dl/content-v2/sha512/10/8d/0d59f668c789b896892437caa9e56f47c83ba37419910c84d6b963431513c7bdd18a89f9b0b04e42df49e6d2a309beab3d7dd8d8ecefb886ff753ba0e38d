            hora: ['1200'],
            id_cita: [''],
            metge_nom: [''],
            centre_nom: [''],
            centre_cp: [''],
            centre_adreca: [''],
            centre_poblacio: [''],
            centre_telefon: [''],
        });
        if (this.edit)
            this.setFormValues();
    }
    setFormValues() {
        let agenda = history.state.agenda;
        let values = {
            title: agenda.title,
            data: agenda.start_date_iso8601,
            location: agenda.site,
            indications: agenda.description,
            agenda_type: agenda.agenda_type_id,
            hora: agenda.start_time.replace(":", "")
        };
        this.form.patchValue(values);
    }
    submit() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            this.showProcessing = true;
            let date = this.form.get('data').value;
            let title = this.form.get('title').value;
            let location = this.form.get('location').value;
            this.form.get('metge_nom').setValue(title);
            this.form.get('centre_nom').setValue(location);
            if (date) {
                var current = new Date(date.split('T')[0]);
                let data_prestacio = this.datepipe.transform(current, 'yyyyMMdd');
                this.form.get('data').setValue(data_prestacio);
            }
            console.log("data: ", { data: this.form.value });
            //const endpoint = this.edit ? 'mgc/agenda':  'mgc/agenda/'+ this.agenda.id;
            if (!this.edit) {
                this.appointment.post('mgc/agenda', { data: this.form.value }).subscribe((data) => Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
                    console.log("data: ", data);
                    this.router.navigate(['/app/home/wellbeing/agenda/success'], { state: { message: 'agenda.add', route: '/app/home/wellbeing/agenda' } });
                    //this.router.navigate(['/app/home/healthcare/appointments/success'], { state: { isOnline: false, appointmentType: 8, isAppointmentChange: false, agenda: data } });
                }), (error) => Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
                    // Called when error
                    console.log('error: ', error);
                    throw new _angular_common_http__WEBPACK_IMPORTED_MODULE_4__["HttpErrorResponse"](error);
                }), () => {
                    // Called when operation is complete (both success and error)
                    // this.showProcessing = false;
                });
            }
            else {
                this.router.navigate(['/app/home/wellbeing/agenda/success'], { state: { message: 'agenda.add', route: '/app/home/wellbeing/agenda' } });
                //this.router.navigate(['/app/home/healthcare/appointments/success'], { state: { isOnline: false, appointmentType: 8, isAppointmentChange: false, agenda: this.agenda } });
