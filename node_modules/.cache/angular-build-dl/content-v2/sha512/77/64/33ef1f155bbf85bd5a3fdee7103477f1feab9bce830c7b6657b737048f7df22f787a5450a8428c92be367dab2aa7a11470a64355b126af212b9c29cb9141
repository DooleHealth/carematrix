te/core';\nimport { Routes, RouterModule } from '@angular/router';\n\nconst routes: Routes = [\n  {\n    path: '',\n    component: IdentityPage\n  }\n];\n@NgModule({\n  imports: [\n    CommonModule,\n    FormsModule,\n    IonicModule,\n    RouterModule.forChild(routes),\n    ReactiveFormsModule,\n    TranslateModule\n  ],\n  \n  declarations: [IdentityPage]\n})\nexport class IdentityPageModule {}\n","import { Component, OnInit } from '@angular/core';\nimport { FormBuilder, FormGroup, Validators } from '@angular/forms';\nimport { AuthenticationService } from '../../../services/authentication.service';\nimport { AlertController, LoadingController } from '@ionic/angular';\nimport { Router } from '@angular/router';\nimport { DatePipe } from '@angular/common';\nimport { TranslateService } from '@ngx-translate/core';\n\n@Component({\n  selector: 'app-identity',\n  templateUrl: './identity.page.html',\n  styleUrls: ['./identity.page.scss'],\n  providers: [DatePipe]\n})\nexport class IdentityPage implements OnInit {\n  \n  credentials: FormGroup;\n  \n  constructor(\n    private fb: FormBuilder,\n    public authService: AuthenticationService,\n    private router: Router,\n    private loadingController: LoadingController,\n    public datepipe: DatePipe,\n    public translate: TranslateService,\n    public alertController: AlertController\n  ) { }\n\n  ngOnInit() {\n    this.credentials = this.fb.group({\n      mutua: this.authService.mutua,\n      tipus_doc: ['DNI', [Validators.required]],\n      num_doc: ['', [Validators.required]],\n      cp: ['', [Validators.required]],\n      data_naixement: ['', [Validators.required]],\n      email: ['']\n    });\n  }\n\n  getToday(): string {\n    return new Date().toISOString().split('T')[0]\n }\n \n  async submit() {\n    const loading = await this.loadingController.create();\n    await loading.present();\n\n    let date = this.credentials.get('data_naixement').value; \n    var current = new Date(date.split('T')[0]);\n    let data_naixement = this.datepipe.transform(current, 'YYYY-MM-dd');\n    this.credentials.get('data_naixement').setValue(data_naixement);\n\n    this.authService.validateCredentials(this.credentials.value, 'registre2').subscribe(\n      async (data) => {\n        await loading.dismiss();\n        this.authService.email = data.email;\n        this.router.navigate(['/email']);\n      },\n      (error) => {\n        // Called when error\n        loading.dismiss();\n        this.showAlert(this.translate.instant('login.fail'));\n        \n      },\n       () => {\n        // Called when operation is complete (both success and error)\n        loading.dismiss();\n      });\n      \n  }\n \nshowAlert(message:string) {\n\n  this.alertController.create({\n    header: 'Error',\n    message: message,\n    buttons: ['OK']\n  }).then(res => {\n\n    res.present();\n\n  });\n\n}  \n\n}\n","export default \"ion-item-divider {\\n  background-color: #d9e7ea;\\n  margin-bottom: 10px;\\n  border-bottom: 1pt solid #006172;\\n}\\n/*# sourceMappingURL=data:application/json;base64, */\";","export default \"<ion-header>\\n  <ion-toolbar>\\n    <ion-buttons slot=\\\"start\\\">\\n      <ion-icon class=\\\"icon_person\\\" src=\\\"assets/icons/usuari.svg\\\"></ion-icon>\\n      <!-- <ion-icon class=\\\"icon_person\\\" name=\\\"person-circle\\\"></ion-icon> -->\\n      <!-- <ion-back-button defaultHref=\\\"/landing\\\"></ion-back-button> -->\\n    </ion-buttons>\\n    <ion-buttons slot=\\\"primary\\\">\\n      <ion-back-button defaultHref=\\\"/credential\\\"></ion-back-button> \\n    </ion-buttons>\\n  \\n    <ion-title >{{ 'landing.registre' | translate }}</ion-title>\\n  </ion-toolbar>\\n</ion-header>\\n\\n<ion-content>\\n  <div class=\\\"landing-container\\\">\\n  <form (ngSubmit)=\\\"submit()\\\" [formGroup]=\\\"credentials\\\">\\n  <div class=\\\"input-group\\\">\\n    <ion-text>\\n      {{ 'identity.text' | translate }}\\n    </ion-text>\\n    <ion-text>\\n      {{ 'identity.text_2' | translate }}\\n    </ion-text><br><br>\\n    <ion-item-divider>\\n        <ion-input formControlName=\\\"num_doc\\\" placeholder=\\\"{{ 'identity.id_placeholder' | translate }}\\\"></ion-input> \\n        <ion-select interface=\\\"popover\\\" value=\\\"DNI\\\" formControlName=\\\"tipus_doc\\\">\\n          <ion-select-option value=\\\"DNI\\\" >DNI</ion-select-option>\\n          <ion-select-option value=\\\"NIF\\\">NIF</ion-select-option>\\n          <ion-select-option value=\\\"NIE\\\">NIE</ion-select-option>\\n        </ion-select>\\n      </ion-item-divider>\\n      <ion-item>\\n        <ion-input formControlName=\\\"cp\\\" placeholder=\\\"{{ 'identity.cp_placeholder' | translate }}\\\"></ion-input>\\n      </ion-item>\\n\\n      <ion-item>\\n        <ion-datetime displayFormat=\\\"DD/MM/YYYY\\\" formControlName=\\\"data_naixement\\\"  [max]=\\\"getToday()\\\" placeholder=\\\"{{ 'identity.birthdate_placeholder' | translate }}\\\"></ion-datetime>\\n      </ion-item>\\n\\n      <ion-item *ngIf=\\\"!this.authService.tiene_email\\\">\\n        <ion-input formControlName=\\\"email\\\" placeholder=\\\"{{ 'identity.email_placeholder' | translate }}\\\"></ion-input>\\n      </ion-item>\\n   \\n      <ion-button type=\\\"submit\\\" color=\\\"primary\\\" expand=\\\"block\\\" [disabled]=\\\"!credentials.valid\\\">\\n      {{ 'identity.button' | translate |Â uppercase}}\\n      </ion-button>\\n  </div>\\n  </form>\\n</div>\\n</ion-content>\\n\";"]}