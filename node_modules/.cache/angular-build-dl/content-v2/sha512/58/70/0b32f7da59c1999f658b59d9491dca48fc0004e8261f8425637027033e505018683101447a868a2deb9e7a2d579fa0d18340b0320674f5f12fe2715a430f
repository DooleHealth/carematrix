('message', async (event) => {\n          this.makeAction(event);\n        });\n      } else {\n        this.message = this.translate.instant('commons.error-network');\n        throwError({ status: 0, message: 'ERR_INTERNET_DISCONNECTED' });\n      }\n    });\n  }\n\n  ngOnInit() {\n    if(this.networkAvailable)\n      this.url = this.symptonCheckerUrl();\n  }\n\n  async makeAction(event) {\n\n    // Post messages from IFrame\n    let action = event.data;\n    if (action == \"cm_urgencies\") {\n      this.router.navigate(['/app/home/healthcare/medical-specialties'], { state: { tipus_registre: 'urg' } });\n    } else if (action == \"telf_24h\") {\n      this.callContact();\n    } else if (action == \"telf_urgencies\") {\n      this.callEmergency();\n    } else if (action == \"videocall\") {\n      this.router.navigate(['/app/home/healthcare/appointments'], { state: { appointmentType: '6', isOnline: true, fromSymptonChecker: true } });\n    } else if (action.method && action.method == \"session\" && action.value?.finalizationPercentage == 100) {\n      // flag used to avoid saving multiple times\n      if (!this.postSessionMessage) {\n        this.postSessionMessage = true;\n        this.authService.post('mgc/symptonChecker', { session: JSON.stringify(action.value), user_id: this.authService.user.id_usuari_amiq }).subscribe(\n          async (data) => {\n\n            console.log(\"response:\", data);\n          },\n          (error) => {\n            // Called when error\n            console.error(error);\n          });\n      } else\n        window.removeEventListener(\"message\", event, true);\n    }\n\n  }\n\n  callContact() {\n\n    console.log(\"calling contact\");\n\n    this.callNumber.callNumber(this.constants.TELEHEALTH_PHONENUMBER, true)\n      .then(res => console.log('Launched dialer!', res))\n      .catch(err => console.log('Error launching dialer', err));\n\n  }\n\n  callEmergency() {\n    console.log(\"calling emergency\");\n\n    this.callNumber.callNumber(this.constants.EMERGENCY_PHONENUMBER, true)\n      .then(res => console.log('Launched dialer!', res))\n      .catch(err => console.log('Error launching dialer', err));\n\n  }\n\n  makeVideoCall() {\n    this.router.navigate(['/app/home/healthcare/appointments'], { state: { appointmentType: '6', isOnline: true, fromSymptonChecker: true } });\n  }\n\n  symptonCheckerUrl() {\n    let lang = this.language.getCurrent();\n    console.log(\"idioma: \", this.language.getCurrent());\n    const endpoint = this.api.getEndpoint('symptonChecker?lang=' + lang);\n    //const endpoint = 'http://192.168.0.158:8000/api/mgc/symptonChecker?lang=' + lang;\n    return this.sanitizer.bypassSecurityTrustResourceUrl(endpoint);\n  }\n\n\n}\n","import { CUSTOM_ELEMENTS_SCHEMA, NgModule } from '@angular/core';\nimport { CommonModule } from '@angular/common';\nimport { FormsModule, ReactiveFormsModule } from '@angular/forms';\n\nimport { IonicModule } from '@ionic/angular';\n\nimport { SymptonCheckerPageRoutingModule } from './sympton-checker-routing.module';\n\nimport { SymptonCheckerPage } from './sympton-checker.page';\nimport { WebView } from '@ionic-native/ionic-webview/ngx';\nimport { TranslateModule } from '@ngx-translate/core';\nimport { CallNumber } from '@ionic-native/call-number/ngx';\nimport { ComponentsModule } from 'src/app/components/components.module';\n\n\n@NgModule({\n  imports: [\n    CommonModule,\n    FormsModule,\n    IonicModule,\n    ReactiveFormsModule,\n    SymptonCheckerPageRoutingModule,\n    TranslateModule,\n    ComponentsModule\n  ],\n  providers:[WebView, CallNumber],\n  declarations: [SymptonCheckerPage]\n})\nexport class SymptonCheckerPageModule {}\n","import { CUSTOM_ELEMENTS_SCHEMA, NgModule } from '@angular/core';\nimport { Routes, RouterModule } from '@angular/router';\n\nimport { SymptonCheckerPage } from './sympton-checker.page';\n\nconst routes: Routes = [\n  {\n    path: '',\n    component: SymptonCheckerPage\n  }\n];\n\n@NgModule({\n  imports: [R