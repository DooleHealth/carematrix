gned\n        if(data.tutor && data.meta){\n         \n          this.appointments.codi_servei = data.meta.metge_codi_especialitat;\n          this.appointments.id_agenda = data.meta.metge_id_agenda;\n          this.appointments.tractament = data.meta.metge_tractament;\n          this.appointments.metgeTutor = data;\n          this.tutor = data.tutor.first_name;\n          this.family_name = data.tutor.last_name;\n          this.address = data.tutor.address;\n        }\n        this.showProcessing = false;\n        \n      },\n      (error) => {\n        // Called when error\n        console.log(\"error: \", error);\n        throw new HttpErrorResponse(error);\n      },\n      () => {\n        // Called when operation is complete (both success and error)\n       \n      });\n  }\n}\n"],"sourceRoot":"webpack:///"}