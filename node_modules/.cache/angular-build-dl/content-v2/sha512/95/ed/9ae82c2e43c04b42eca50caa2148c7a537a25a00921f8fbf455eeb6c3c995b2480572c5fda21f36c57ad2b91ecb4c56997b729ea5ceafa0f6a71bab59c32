slider mevaSalut\nasync getDato(){\n  \n  this.authService.get('mgc/indicadorsSalut').pipe(\n    mergeMap((v) => !v.success ? throwError(v) : of(v)) \n  ).subscribe(\n      async (data) => {\n        this.dato = data;    \n        this.cardio = this.dato.cardio;\n        this.exercici = this.dato.exercici;\n        this.habits = this.dato.habits;\n        this.active = this.dato.active;\n\n        if (this.dato === true){\n          this.options[0].route = 'dato';\n        }\n      },\n      (error) => {\n        // Called when error\n        if(error.message == 'ERR_INTERNET_DISCONNECTED' || error.message == 'Timeout has occurred' ){\n          this.notifyUser();\n        }else\n          throw error;\n      });\n}\n\nnotifyUser(){\n  const config = new MatSnackBarConfig();\n        config.panelClass = ['example-pizza-party'];\n        config.verticalPosition = 'bottom';\n        config.horizontalPosition = 'center';\n        config.duration = 3000;\n        this.snackBar.open(this.translate.instant('commons.error-network'),'', config);\n}\nasync getAgenda(){\n  //const loading = await this.loadingController.create();\n  //await loading.present();\n\n  this.authService.get('user/agenda').subscribe(\n      async (data) => {\n       \n        this.authService.agendaUser = [];\n        this.appointmentService.agendas = data;\n        this.authService.agendaUser = data.agenda;\n        \n        // remove agenda if it's set to hide\n        this.authService.agendaUser?.forEach( function(element, index, object) {\n          if(element.hide){\n            object.splice(index, 1);\n          }\n        });\n\n        if(this.authService.agendaUser?.length > 0 && this.authService.agendaUser.length == 0)\n          console.log(\"all agendas are hidden\");\n       \n      },\n      (error) => {\n        // Called when error\n        throw error;\n      });\n}\n\nclose(agenda){\n \n  this.authService.post('user/agenda/'+agenda.id+'/hide', {}).subscribe(\n    async (data) => {\n      this.authService.agendaUser = this.authService.agendaUser.filter(obj => obj !== agenda);\n    },\n    (error) => {\n      // Called when error\n      throw error;\n    },\n    () => {\n      // Called when operation is complete (both success and error)\n      \n    });\n};\n\n\n}\n"]}