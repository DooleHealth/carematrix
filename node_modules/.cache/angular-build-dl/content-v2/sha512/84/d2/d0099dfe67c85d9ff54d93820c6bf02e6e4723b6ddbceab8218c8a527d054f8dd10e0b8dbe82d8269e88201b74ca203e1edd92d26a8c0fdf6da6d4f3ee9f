   this.credentials = this.fb.group({\n      id_usuari_mgc: [this.authService.id_usuari_mgc, [Validators.required]]\n    });\n  }\n\n  async submit() {\n    const loading = await this.loadingController.create();\n    await loading.present();\n   \n    this.authService.validateCredentials(this.credentials.value, 'login2').subscribe(\n      async (data) => {\n        await loading.dismiss();\n        this.router.navigate(['/app/home']);\n       \n      },\n      (error) => {\n        // Called when error\n        loading.dismiss();\n        this.showAlert(this.translate.instant('login.fail'));\n        \n      },\n       () => {\n        // Called when operation is complete (both success and error)\n        loading.dismiss();\n      });\n      \n  }\n  \nshowAlert(message:string) {\n\n  this.alertController.create({\n    header: 'Error',\n    message: message,\n    buttons: ['OK']\n  }).then(res => {\n\n    res.present();\n\n  });\n\n}  \n\n}\n","export default \"<ion-header>\\n  <ion-toolbar>\\n    <ion-buttons slot=\\\"start\\\">\\n      <ion-icon class=\\\"icon_person\\\" src=\\\"assets/icons/usuari.svg\\\"></ion-icon>\\n    </ion-buttons>\\n    <ion-back-button\\n      slot=\\\"end\\\"\\n      text=\\\"\\\"\\n      defaultHref=\\\"/landing\\\"\\n    ></ion-back-button>\\n\\n    <ion-title>{{ 'legal.header' | translate }}</ion-title>\\n  </ion-toolbar>\\n</ion-header>\\n\\n<ion-content fullscreen style=\\\"background: #d9e7ea\\\">\\n  <form (ngSubmit)=\\\"submit()\\\" lines=\\\"none\\\">\\n    <div class=\\\"input-group\\\" style=\\\"background: #d9e7ea\\\">\\n      <ion-item lines=\\\"none\\\">\\n        <ion-text><div [innerHTML] =\\\"this.agreementText\\\">\\n\\n        </div>\\n      </ion-text>\\n      </ion-item>\\n    </div>\\n    <ion-row\\n      ><ion-col col-6>\\n        <ion-button type=\\\"submit\\\" expand=\\\"block\\\" color=\\\"primary\\\">\\n          {{ 'legal.ok_button' | translate | uppercase}}</ion-button\\n        ></ion-col\\n      ><ion-col c